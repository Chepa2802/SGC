                                <label class="input">
                                    @Html.TextBoxFor(m => m.mme.me_conductor.e_empresa_trans.vc_desc_empresa_trans,
                                    new
                                    {
                                        atributo    = "mme.me_conductor.e_empresa_trans.vc_desc_empresa_trans",
                                        id          = "vc_desc_empresa_trans",
                                        disabled    = "disabled"
                                    })
                                </label>
                            </div>
                        </section>
