                                        id          = "vc_direccion",
                                        disabled    = "disabled"
                                    })
                                </label>
                            </div>
                        </section>
                        <section class="smart-form col-sm-4 col-md-4 col-lg-4">
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label class="label-margin">
                                    Grupo Sanguineo
                                </label>
                            </div>
                            <div class="col-sm-8 col-md-8 col-lg-8">
                                <label class="input">
                                    @Html.TextBoxFor(m => m.mme.me_conductor.e_grupo_sanguineo.vc_desc_grupo_sanguineo,
                                    new
                                    {
                                        atributo    = "mme.me_conductor.e_grupo_sanguineo.vc_desc_grupo_sanguineo",
                                        id          = "vc_desc_grupo_sanguineo",
                                        disabled    = "disabled"
                                    })
                                </label>
                            </div>
                        </section>
                    </div>

                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <section class="smart-form col-sm-4 col-md-4 col-lg-4">
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label class="label-margin">
                                    Restricciones
                                </label>
                            </div>
                            <div class="col-sm-8 col-md-8 col-lg-8">
                                <label class="input">
                                    @Html.TextBoxFor(m => m.mme.me_conductor.e_conductor.vc_restricciones,
                                    new
                                    {
                                        atributo    = "mme.me_conductor.e_conductor.vc_restricciones",
                                        id          = "vc_restricciones",
                                        disabled    = "disabled"
                                    })
                                </label>
                            </div>
                        </section>
                        <section class="smart-form col-sm-4 col-md-4 col-lg-4">
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label class="label-margin">
                                    Fecha Inicio
                                </label>
                            </div>
                            <div class="col-sm-8 col-md-8 col-lg-8">
                                <label class="input">
                                    <i class="icon-append fa fa-calendar"></i>
                                    @Html.TextBoxFor(m => Model.mme.me_conductor.e_conductor.dt_fec_inicio, String.Format("{0:dd/MM/yyyy}", Model.mme.me_conductor.e_conductor.dt_fec_inicio),
                                    new
                                    {
                                        type        = "text",
                                        disabled    = "disabled",
                                        id          = "dt_fec_inicio"
                                    })
                                </label>
                            </div>
                        </section>
                        <section class="smart-form col-sm-4 col-md-4 col-lg-4">
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label class="label-margin">
                                    Fecha Final
                                </label>
                            </div>
                            <div class="col-sm-8 col-md-8 col-lg-8">
                                <label class="input">
                                    <label class="input">
                                        <i class="icon-append fa fa-calendar"></i>
                                        @Html.TextBoxFor(m => Model.mme.me_conductor.e_conductor.dt_fec_final, String.Format("{0:dd/MM/yyyy}", Model.mme.me_conductor.e_conductor.dt_fec_final),
                                        new
                                        {
                                            type        = "text",
                                            disabled    = "disabled",
                                            id          = "dt_fec_final"
                                        })
                                    </label>
                                </label>
                            </div>
                        </section>
                    </div>

