@model SGC.Areas.Global.Models.ConductorModel
@{
    ViewBag.Title = "SGC | Visualizar Carné";
    Layout = "~/Areas/Sistema/Views/MP_Vista.cshtml";
}

<div id="main" role="main">
    <div id="ribbon">
        <ol class="breadcrumb">
            <img src="/Recursos/Img/IconoPagina.png" style="width: 35px" />&nbsp&nbsp
            <li><b>Información de Carné</b></li>
        </ol>
    </div>

    <div id="content">
        <div class="well NoPadding">
            <button class="btn btn-primary" onclick="document.location = '@Url.RouteUrl("V_Sel_ConsultaCarne")';">
