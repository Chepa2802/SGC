                        <section class="smart-form col-sm-4 col-md-4 col-lg-4">
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label class="label-margin">
                                    Clase Licencia
                                </label>
                            </div>
                            <div class="col-sm-8 col-md-8 col-lg-8">
                                <label class="input">
                                    @Html.TextBoxFor(m => m.mme.me_conductor.e_clase_licencia.vc_desc_clase_licencia,
                                    new
                                    {
                                        atributo    = "mme.me_conductor.e_clase_licencia.vc_desc_clase_licencia",
                                        id          = "vc_desc_clase_licencia",
                                        disabled    = "disabled"
                                    })
                                </label>
                            </div>
