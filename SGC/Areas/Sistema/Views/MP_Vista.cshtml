<body class="desktop-detected smart-style-2 pace-done">

    <header id="header">
        <div id="logo-group" style="background-color:White;border:1px #e7e7e7 solid;">
            <span id="logo" style="margin:0px;">

                <a href="#"><img src="~/Recursos/Logos/IconoLogo.png" alt="TISI" style="height:40px;width:40px;margin-top:5px;"></a>

            </span>



        </div>
        <div class="project-context ">
            <a><h1><b>@Sol.vc_desc_proyecto</b></h1></a>
        </div>

        <div class="pull-right">
            <div id="hide-menu" class="btn-header pull-right">
                <span> <a href="javascript:void(0);" data-action="toggleMenu" title="Collapse Menu"><i class="fa fa-reorder"></i></a> </span>
            </div>
            <ul id="mobile-profile-img" class="header-dropdown-list hidden-xs padding-5">
                <li class="">
                    <a href="#" class="dropdown-toggle no-margin userdropdown" data-toggle="dropdown">
                        <img src="/Temas/Smart/img/avatars/sunny.png" alt="John Doe" class="online" />
                    </a>
                    <ul class="dropdown-menu pull-right">
                        <li>
                            <a href="javascript:void(0);" class="padding-10 padding-top-0 padding-bottom-0"><i class="fa fa-cog"></i> Setting</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="profile.html" class="padding-10 padding-top-0 padding-bottom-0"> <i class="fa fa-user"></i> <u>P</u>rofile</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="javascript:void(0);" class="padding-10 padding-top-0 padding-bottom-0" data-action="toggleShortcut"><i class="fa fa-arrow-down"></i> <u>S</u>hortcut</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="javascript:void(0);" class="padding-10 padding-top-0 padding-bottom-0" data-action="launchFullscreen"><i class="fa fa-arrows-alt"></i> Full <u>S</u>creen</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="login.html" class="padding-10 padding-top-5 padding-bottom-5" data-action="userLogout"><i class="fa fa-sign-out fa-lg"></i> <strong><u>L</u>ogout</strong></a>
                        </li>
                    </ul>
                </li>
            </ul>

            <div id="logout" class="btn-header transparent pull-right">
                <span> <a href="@Url.RouteUrl("AC_Salir")" title="Salir del sistema"><i class="fa fa-sign-out"></i></a> </span>
            </div>

            <div id="search-mobile" class="btn-header transparent pull-right">
                <span> <a href="javascript:void(0)" title="Search"><i class="fa fa-search"></i></a> </span>
            </div>

            <form action="search.html" class="header-search pull-right">
                <input id="Buscar" type="text" name="param" placeholder="Buscar...">
                <button type="submit" onclick="return false;">
                    <i class="fa fa-search" onclick="AC_Busqueda_Preferencial();"></i>
                </button>
                <a href="javascript:void(0);" id="cancel-search-js" title="Cancel Search"><i class="fa fa-times"></i></a>
            </form>

            <div class="btn-header transparent pull-right">
                <span> <a href="javascript:void(0);" data-action="launchFullscreen" title="Full Screen"><i class="fa fa-arrows-alt"></i></a> </span>
            </div>

            <div hidden="hidden" id="speech-btn" class="btn-header transparent pull-right hidden-sm hidden-xs">
                <div>
                    <a href="javascript:void(0)" id="Micro" onclick="Escuchar();"><i class="fa fa-microphone"></i></a>
                </div>
            </div>
        </div>

    </header>

    <aside id="left-panel" style="background:white;border:1px #e7e7e7 solid;">


        <div class="col-sm-12 col-md-12 col-lg-12 login-info cursorMano" id="show-shortcut" data-action="toggleShortcut">
            <span style="height:25px;padding:3px;">
                <a href="javascript:void(0);">
                    <span style="color:#5ea100">
                        <img src="/Recursos/Img/Red-Global.gif" style="max-width:60px" /> <b> @Sol.vc_usuario </b>
                    </span>
                </a>
            </span>
        </div>
        <hr />

        <nav id="menu-general" style="margin-top:56px;">
            <ul>
                @*CONSULTAS*@
                <li class="menu-nivel-2">
                    <a href="#" title="Consultas"><i class="fa fa-lg fa-fw fa-taxi"></i> <span class="menu-item-parent">CONSULTAS</span></a>
                    <ul>
                        <li class="menu-nivel-1">
                            <a href="#" title="Carnés"><span class="menu-item-parent">CARNÉS</span></a>
                            <ul>
                                <li>
                                    <a href="@Url.RouteUrl("V_Sel_ConsultaCarne")">CONSULTAR</a>
                                </li>
                                <li>
                                    <a href="@Url.RouteUrl("V_Sel_ImpresionCarne")">IMPRIMIR</a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </li>
                
                @*CONFIGURACIONES*@
                <li class="menu-nivel-2">
                    <a href="#" title="Consultas"><i class="fa fa-lg fa-fw fa-gears"></i> <span class="menu-item-parent">CONFIGURACIÓN</span></a>
                    <ul>
                        <li class="menu-nivel-1">
                            <a href="#" title="Empresa de Transporte"><span class="menu-item-parent">EMPRESA DE TRANSPORTE</span></a>
                            <ul>
                                <li>
                                    <a href="@Url.RouteUrl("V_Sel_ConsultaEmpresaTrans")">CONSULTAR</a>
                                </li>
                                <li>
                                    <a href="@Url.RouteUrl("V_Ins_EmpresaTrans")">REGISTRAR</a>
                                </li>
                            </ul>
                        </li>
                        <li class="menu-nivel-1">
                            <a href="#" title="Conductores"><span class="menu-item-parent">CONDUCTORES</span></a>
                            <ul>
                                <li>
                                    <a href="@Url.RouteUrl("V_Sel_Conductor")">CONSULTAR</a>
                                </li>
                                <li>
                                    <a href="@Url.RouteUrl("V_Ins_Conductor")">REGISTRAR</a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </li>
            </ul>
        </nav>

        <span class="minifyme" data-action="minifyMenu">
            <i class="fa fa-arrow-circle-left hit"></i>
        </span>

    </aside>

    @RenderBody()

    <footer class="text-center" style="background:White;border-bottom-left-radius:8px;border-bottom-right-radius:8px;">
        <div class="col-sm-12 col-md-12 col-lg-12 text-center">
