    <script type="text/javascript">

        $(document).ready(function () {
            pageSetUp();

            if ($('body').attr('class').search('minified') > -1 || $('body').attr('class').search('menu-on-top') > -1) {
                $('#Contenido_Info').attr('style', 'display:none');
            }
            else {
                $('#Contenido_Info').removeAttr('style');
            }

            //Menu();
        });

        $(document).on('click', '.minifyme', function () {
            if ($('body').attr('class').search('minified') > -1 || $('body').attr('class').search('menu-on-top') > -1) {
                $('#Contenido_Info').attr('style', 'display:none');
            }
            else {
                $('#Contenido_Info').removeAttr('style');
            }

        });

        function Menu() {
            alert('entra')
            $('.no-item').remove();

            $('body .menu-nivel-1').each(function () {

                if ($(this).find('ul').eq(0).text().trim() == '') {
                    $(this).remove();
                }

            });

            $('body .menu-nivel-2').each(function () {

                if ($(this).find('ul').eq(0).text().trim() == '') {
                    $(this).remove();
                }

            });
            $('body .menu-nivel-3').each(function () {

                if ($(this).find('ul').eq(0).text().trim() == '') {
                    $(this).remove();
                }

            });

            $('#menu-general').removeAttr('hidden')

        }
    </script>


