        $(document).on('keypress', '#Buscar', function (evt) {
            if (evt.which == 13) {
                AC_Busqueda_Preferencial();
                return false;
            }
        });

        $(document).on('click', '.minifyme', function () {
            if ($('body').attr('class').search('minified') > -1 || $('body').attr('class').search('menu-on-top') > -1) {
